(defproject empire "0.3.0"
  :description "Clojure client for the Empire API. Empire is an API for accessing enterprise SaaS services such as Salesforce, Zendesk, Google Apps, etc. Empire provides a uniform, database-like interface to every service that it supports. Empire makes it easy to integrate data from multiple enterprise services into your own enterprise app."
  :url "http://empiredata.co"
  :license {:name "Apache License, Version 2.0"
            :url "http://www.apache.org/licenses/LICENSE-2.0.html"}
  :dependencies [[org.clojure/clojure "1.6.0"]
                 [org.clojure/data.json "0.2.5"]
                 [clj-http "1.0.0"]
                 [clj-http-fake "0.7.8"]
                 [clj-time "0.8.0"]
                 [clj-yaml "0.4.0"]]
  :plugins [[lein-exec "0.3.4"]]
  :signing {:gpg-key "alex@upshotdata.com"})
