  :repositories {"nuition" {:url "http://maven.nuiton.org/release"}}
  :dependencies [[org.clojure/clojure "1.5.1"]
                 [cheshire "5.1.1"]
                 [com.rabbitmq/amqp-client "2.5.0"]
