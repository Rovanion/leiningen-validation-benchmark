(defproject org.clojars.oakes/rsyntaxtextarea "2.5.2"
  :description "A syntax highlighting, code folding text editor for Java Swing applications"
  :url "https://github.com/oakes/RSyntaxTextArea"
  :license {:name "Modified BSD License"
            :url "http://fifesoft.com/rsyntaxtextarea/RSyntaxTextArea.License.txt"}
  :resource-paths ["src"]
  :java-source-paths ["src"]
  :javac-options ["-target" "1.6" "-source" "1.6" "-Xlint:-options"]
  :aot :all
  :omit-source true
  :jar-exclusions [#"^.*\.(java|flex)$"]
  :dependencies [])
