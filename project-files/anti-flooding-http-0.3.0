(defproject truckerpath/anti-flooding-http "0.3.0"
  :description "Wrapper around clj-http with anti-flooding magic (prevents identical simultaneous requests)"
  :url "https://github.com/truckerpathteam/anti-flooding-http"
  :license {:name "MIT License"
            :url "https://github.com/truckerpathteam/anti-flooding-http/blob/master/LICENSE"}
  
  :dependencies [[org.clojure/clojure "1.8.0" :scope "provided"]
                 [org.clojure/tools.logging "0.3.1"]
                 [clj-http "2.0.0"]]

  :profiles {:dev {:dependencies [[slingshot "0.12.2"]]}}

  :deploy-repositories [["clojars" {:creds :gpg}]])
