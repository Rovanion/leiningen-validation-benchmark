(defproject org.clojars.toxi/clj-json "0.5.0"
  :description "Fast JSON encoding and decoding for Clojure via the Jackson library (patched to use Jackson 1.9.4)."
  :url "http://github.com/mmcgrana/clj-json"
  :source-path "src/clj"
  :java-source-path "src/jvm"
  :javac-fork "true"
  :dependencies [[org.clojure/clojure "1.3.0"]
                 [org.codehaus.jackson/jackson-core-asl "1.9.4"]]
  :dev-dependencies [[lein-clojars "0.7.0"]])
