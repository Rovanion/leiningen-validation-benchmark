(defproject racehub/atlas "0.1.4"
  :description "Utilities for creating maps in Clojurescript using the
  Google Maps API and Om."
  :url "http://github.com/racehub/atlas"
  :license {:name "Eclipse Public License"
            :url "http://www.eclipse.org/legal/epl-v10.html"}
  :min-lein-version "2.3.0"
  :jar-exclusions [#".DS_Store"]
  :dependencies [[om "0.8.0-beta1"]
                 [org.clojure/clojure "1.6.0"]
                 [org.clojure/clojurescript "0.0-2371"]
                 [org.clojure/core.async "0.1.346.0-17112a-alpha"]
                 [prismatic/om-tools "0.3.6" :exclusions [om]]
                 [prismatic/schema "0.3.1"
                  :exclusions [org.clojure/clojurescript]]
                 [racehub/om-bootstrap "0.3.1"
                  :exclusions [org.clojure/clojure om]]
                 [racehub/util "0.2.13"
                  :exclusions [org.clojure/clojure com.taoensso/encore]]])
