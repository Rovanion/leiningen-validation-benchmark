  :dependencies [[org.clojure/clojure "1.2.0"]
                 [org.clojure/clojure-contrib "1.2.0"]
                 [plugboard "1.2.0"]
                 ]

  :dev-dependencies [
                     [swank-clojure "1.2.1"]
                     [hiccup "0.2.6"
