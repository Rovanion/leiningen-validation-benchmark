(defproject antistock/rabbitmq "0.2.6"
  :description "Antistock component library"
  :url "https://bitbucket.org/antistock/component"
  :license {:name "Creative Commons Attribution-NonCommercial 3.0"
            :url "http://creativecommons.org/licenses/by-nc/3.0/"}
  :deploy-repositories [["releases" :clojars]]
  :min-lein-version "2.0.0"
  :dependencies [[antistock/component "0.2.0"]
                 [kithara "0.1.4"]
                 [org.clojure/clojure "1.8.0"]
                 [prismatic/schema "1.1.1"]
                 [org.clojure/tools.logging "0.3.1"]
                 [noencore "0.3.1"]]
  :aliases {"ci" ["do" ["difftest"] ["lint"]]
            "lint" ["do" ["whitespace-linter"] ["eastwood"]]}
  :profiles {:dev {:dependencies [[ch.qos.logback/logback-classic "1.1.7"]]
                   :plugins [[jonase/eastwood "0.2.3"]
                             [lein-difftest "2.0.0"]
                             [listora/whitespace-linter "0.1.0"]]}})
